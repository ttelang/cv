{
  "name": "career-services-platform",
  "version": "1.0.0",
  "description": "Enterprise-grade career services platform with AI-powered features",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "build": "npm run build:services",
    "build:services": "npm run build:user && npm run build:resume && npm run build:job && npm run build:coaching",
    "build:user": "cd services/user-service && npm run build",
    "build:resume": "cd services/resume-service && npm run build",
    "build:job": "cd services/job-service && npm run build",
    "build:coaching": "cd services/coaching-service && npm run build",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "migrate:up": "knex migrate:latest",
    "migrate:down": "knex migrate:rollback",
    "seed": "knex seed:run",
    "deploy:staging": "npm run build && npm run deploy:k8s:staging",
    "deploy:production": "npm run build && npm run deploy:k8s:production",
    "deploy:k8s:staging": "kubectl apply -f k8s/staging/",
    "deploy:k8s:production": "kubectl apply -f k8s/production/"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^6.8.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "passport": "^0.6.0",
    "passport-oauth2": "^1.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "mongodb": "^5.7.0",
    "redis": "^4.6.7",
    "elasticsearch": "^16.7.3",
    "kafka-node": "^5.0.0",
    "aws-sdk": "^2.1434.0",
    "@aws-sdk/client-s3": "^3.385.0",
    "@aws-sdk/client-ses": "^3.385.0",
    "stripe": "^12.18.0",
    "nodemailer": "^6.9.4",
    "socket.io": "^4.7.2",
    "joi": "^17.9.2",
    "winston": "^3.10.0",
    "prom-client": "^14.2.0",
    "opentelemetry": "^1.15.2",
    "tensorflow": "^4.9.0",
    "transformers": "^2.3.0",
    "spacy": "^0.2.1",
    "pdf-lib": "^1.17.1",
    "puppeteer": "^21.0.1",
    "sharp": "^0.32.4",
    "multer": "^1.4.5-lts.1",
    "mime-types": "^2.1.35",
    "validator": "^13.11.0",
    "moment": "^2.29.4",
    "lodash": "^4.17.21",
    "uuid": "^9.0.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.6.2",
    "supertest": "^6.3.3",
    "eslint": "^8.46.0",
    "prettier": "^3.0.1",
    "@types/node": "^20.4.9",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "typescript": "^5.1.6",
    "ts-node": "^10.9.1",
    "docker-compose": "^0.24.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/career-platform/career-services"
  },
  "keywords": [
    "career",
    "resume",
    "jobs",
    "ai",
    "microservices",
    "enterprise",
    "coaching",
    "platform"
  ],
  "author": "Career Platform Team",
  "license": "MIT",
  "workspaces": [
    "services/*",
    "packages/*",
    "apps/*"
  ]
}
