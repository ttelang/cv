{
  "name": "cv-generator",
  "version": "1.0.0",
  "description": "Dynamic resume generator from JSON Resume schema",
  "main": "resume-generator/resume-generator.js",
  "scripts": {
    "build": "node resume-generator/build-resume.js build",
    "build:jpmc": "node resume-generator/build-resume.js build jpmc",
    "build:standard": "node resume-generator/build-resume.js build standard",
    "build:oreilly": "node resume-generator/build-resume.js build oreilly",
    "build:proptech": "node resume-generator/build-resume.js build proptech",
    "build:user": "node resume-generator/build-resume.js --user",
    "build:tarun:jpmc": "node resume-generator/build-resume.js --user=tarun --target=jpmc --role=director",
    "build:tarun:tech": "node resume-generator/build-resume.js --user=tarun --theme=tech",
    "build:kriti:lloyds": "node resume-generator/build-resume.js --user=kriti --target=lloyds",
    "build:all": "node resume-generator/build-resume.js --all-users",
    "compile:json": "node resume-generator/compile-resume.js --user=tarun",
    "validate": "node resume-generator/validate-data.js",
    "jobs:list": "node resume-generator/job-cli.js list",
    "jobs:analyze": "node resume-generator/job-cli.js analyze",
    "jobs:suggest": "node resume-generator/job-cli.js suggest", 
    "jobs:keywords": "node resume-generator/job-cli.js keywords",
    "jobs:validate": "node resume-generator/job-cli.js validate",
    "serve": "python3 -m http.server 8000",
    "dev": "npm run build:jpmc && npm run serve"
  },
  "keywords": [
    "resume",
    "cv",
    "json-resume",
    "generator",
    "html"
  ],
  "author": "Tarun Telang",
  "license": "MIT",
  "devDependencies": {},
  "dependencies": {}
}
